{
    "manifest_version": 3,
    "name": "Facebook Auto Appearance",
    "version": "0.1",
    "background": {
        "service_worker": "backgroundScript.js"
      },
      "permissions": [
        "scripting",
        "activeTab"
      ],
    "content_scripts": [{
        "matches": ["https://www.facebook.com/*"],
        "js": ["script.js"] 
    }]
}
