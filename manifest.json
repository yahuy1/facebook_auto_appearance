{
    "manifest_version": 2,
    "name": "My First Extension",
    "version": "0.1",
    "background": {
        "scripts": ["backgroundScript.js"]
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["https://www.facebook.com/*"],
        "js": ["script.js"]
    }]
}